run = "python scheduler.py"
language = "python3"
entrypoint = "scheduler.py"
modules = ["python-3.12", "bash", "nix"]

[nix]
channel = "stable-25_05"

[env]
PYTHONPATH = "${PYTHONPATH}:${HOME}/${REPL_SLUG}"

[packager]
language = "python3"

[packager.features]
enabledForHosting = false
packageSearch = true
guessImports = true

[deployment]
run = ["sh", "-c", "python scheduler.py"]
